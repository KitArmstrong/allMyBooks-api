{
  "name": "all-my-books-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf ./dist && tsc -p tsconfig.build.json && tscpaths -p tsconfig.build.json -s ./src -o ./dist",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest --ci --runInBand",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:unit": "jest --testRegex unit",
    "test:e2e": "jest --testRegex e2e --runInBand",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "clear-db": "node dist/_scripts/clear-db.js",
    "seed-db": "node dist/_scripts/seed-db.js",
    "migrate-db": "node dist/_scripts/migrate-db.js"
  },
  "dependencies": {
    "@nestjs/common": "6.5.3",
    "@nestjs/core": "6.5.3",
    "@nestjs/passport": "6.1.0",
    "@nestjs/platform-express": "6.5.3",
    "@nestjs/swagger": "3.1.0",
    "@nestjs/typeorm": "6.1.3",
    "bcryptjs": "2.4.3",
    "class-transformer": "0.2.2",
    "class-validator": "0.9.1",
    "dotenv": "7.0.0",
    "faker": "4.1.0",
    "jsonwebtoken": "9.0.0",
    "pg": "7.10.0",
    "reflect-metadata": "0.1.12",
    "rxjs": "6.3.3",
    "swagger-ui-express": "4.0.5",
    "typeorm": "0.2.17",
    "typeorm-seeding": "1.0.0-beta.6"
  },
  "devDependencies": {
    "@nestjs/testing": "6.2.0",
    "@types/bcryptjs": "2.4.2",
    "@types/express": "4.16.0",
    "@types/faker": "4.1.5",
    "@types/jest": "23.3.13",
    "@types/jsonwebtoken": "8.3.2",
    "@types/node": "10.12.18",
    "@types/supertest": "2.0.7",
    "jest": "24.8.0",
    "jest-junit": "6.4.0",
    "nodemon": "1.19.1",
    "prettier": "1.15.3",
    "supertest": "3.4.1",
    "ts-jest": "23.10.5",
    "ts-node": "7.0.1",
    "tsconfig-paths": "3.7.0",
    "tscpaths": "0.0.9",
    "tslint": "5.12.1",
    "typescript": "3.2.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "^src(.*)$": "<rootDir>/src/$1"
    },
    "reporters": [
      "default",
      "jest-junit"
    ],
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./test-results/coverage",
    "testEnvironment": "node"
  },
  "jest-junit": {
    "outputDirectory": "./test-results"
  }
}
